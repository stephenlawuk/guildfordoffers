<div id="notice"><%= notice %></div>

<h1>Listing users</h1>

<% @users.each do |user| %>
<table class="userlisting">
  <tr>
    <td><b>Username</b></td>
    <td><%= user.username %></td>
  </tr>
  <tr>
    <td><b>Crypted password</b></td>
    <td><%= user.crypted_password %></td>
  </tr>
  <tr>
    <td><b>Password salt</b></td>
    <td><%= user.password_salt %></td>
  </tr>
  <tr>
    <td><b>Persistence token</b></td>
    <td><%= user.persistence_token %></td>
  </tr>
  <tr>
    <td colspan="2">
    <%= link_to 'Show', user %> |
    <%= link_to 'Edit', edit_user_path(user) %> |
    <%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %>
    </td>
  </tr>
</table>
<br/>
<% end %>


<br />

<%= link_to 'New User', new_user_path %>
